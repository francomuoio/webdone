<%= simple_form_for User.new,
  :url => users_create_path, method: 'GET' do |f| %>
  <%= f.input :email %>
    <!--
    Input avec Projet ID -> retrouver le projet
    -->
  <%= f.input :repos, collection: @repolist %>

  <%= f.submit "Inviter un client", class: "btn btn-gray-light btn-lg" %>
  <%= link_to "Back", projets_path, class: "btn btn-gray-light btn-lg" %>
<% end %>

